<resources>
	<properties>
		<property id="recordData" type="number"><!--Value 0 means don't record, otherwise record to FIT file-->1</property>
	</properties>
	<settings>
		<setting propertyKey="@Properties.recordData" title="@Strings.recordTitle">
			<settingConfig type="list">
				<listEntry value="0">@Strings.recordOff</listEntry>
				<listEntry value="1">@Strings.recordOn</listEntry></settingConfig>
		</setting>
	</settings>
</resources>